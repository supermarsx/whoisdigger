{
  "name": "whoisdigger",
  "version": "0.1.0",
  "description": "Bulk whois lookup tool",
  "main": "./dist/app/js/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "debug-powershell": "@powershell -Command $env:DEBUG='*';npm start;",
    "debug-cmd": "set DEBUG=* & npm start",
    "update-bulma-w": "copy node_modules\\bulma\\css\\bulma.min.css app\\css\\bulma.min.css",
    "update-bulma-x": "cp node_modules/bulma/css/bulma.min.css app/css/bulma.min.css",
    "package-mac": "npm run build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/icons/app.icns --prune=true --out=release_builds",
    "package-win": "npm run build && electron-packager . whoisdigger --overwrite --asar --platform=win32 --arch=ia32 --icon=app/icons/app.ico --prune=true --out=release_builds --version-string.CompanyName=\"WhoisTeam\" --version-string.FileDescription=\"WhoisTeam Whoisdigger\" --version-string.ProductName=\"whoisdigger\"",
    "package-linux": "npm run build && electron-packager . whoisdigger --overwrite --asar --platform=linux --arch=x64 --icon=app/icons/app.png --prune=true --out=release_builds",
    "package-all": "npm run build && electron-packager . --all --overwrite --asar --icon=app/icons/app.png --prune=true --out=release_builds",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint app/js/common/conversions.ts test/**/*.ts",
    "test": "jest"
  },
  "author": "Eduardo Mota",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.9.2",
    "change-case": "^4.1.2",
    "cp": "^0.2.0",
    "csv": "^5.4.0",
    "datatables": "^1.10.18",
    "debug": "^4.3.1",
    "dedent-js": "^1.0.1",
    "electron-packager": "^15.2.0",
    "electron-store": "^5.2.0",
    "html-entities": "^1.4.0",
    "idna-uts46": "^1.1.0",
    "jquery": "^3.6.0",
    "jszip": "^3.7.0",
    "papaparse": "^5.3.0",
    "psl": "^1.8.0",
    "punycode": "^2.1.1",
    "snyk": "^1.667.0",
    "whois": "^2.13.0"
  },
  "snyk": true,
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jquery": "^3.5.32",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "electron": "^12.0.1",
    "eslint": "^9.29.0",
    "jest": "^30.0.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
