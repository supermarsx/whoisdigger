<div id="singlewhoisSearchContainer" class="field has-addons box-shadow-5">
  <p class="control is-large is-expanded">
    <input
      id="singlewhoisSearchInputDomain"
      class="input is-large"
      type="text"
      placeholder="Domain name (ex.: whoisteam.com)"
    />
  </p>
  <p class="control">
    <a id="singlewhoisSearchButtonSearch" class="button is-large"><i class="fas fa-search"></i></a>
  </p>
</div>

<div id="singlewhoisMessageError" class="notification is-danger box-shadow-5 is-hidden">
  <button data-notif="singlewhoisMessageError" class="delete"></button> Whois lookup failed, an
  error has ocurred. Check your internet connection, domain validity, amount of whois requests or
  whois server. <a class="singlewhoisMessageWhoisOpen" href="#">Check response.</a>
</div>

<div id="singlewhoisMessageAvailable" class="notification is-success box-shadow-5 is-hidden">
  <button data-notif="singlewhoisMessageAvailable" class="delete"></button> Domain is available for
  registry. <a class="singlewhoisMessageWhoisOpen" href="#">Check the whois request response.</a>
</div>

<div id="singlewhoisMessageUnavailable" class="notification is-danger box-shadow-5 is-hidden">
  <button data-notif="singlewhoisMessageUnavailable" class="delete"></button> Domain is unavailable
  for registry.
  <a class="singlewhoisMessageWhoisOpen" href="#">Check the whois request response.</a>
</div>

<div id="singlewhoisModals">
  <div id="singlewhoisMessageWhois" class="modal">
    <div class="modal-background"></div>
    <div class="modal-content">
      <div class="box">
        <div class="highlight-full">
          <figure class="highlight">
            <pre id="singlewhoisMessageWhoisResults" class="has-text-left"></pre>
          </figure>
        </div>
      </div>
    </div>
    <button
      id="singlewhoisMessageWhoisClose"
      class="modal-close is-large"
      aria-label="close"
    ></button>
  </div>

  <div id="singlewhoisDomainCopied" class="modal">
    <div class="modal-background"></div>
    <div class="modal-content">
      <div class="box">
        <div class="highlight-full">
          <figure class="highlight">
            <pre class="has-text-left">
              The domain url has been copied
            </pre>
          </figure>
        </div>
      </div>
    </div>
    <button
      id="singlewhoisDomainCopiedClose"
      class="modal-close is-large"
      aria-label="close"
    ></button>
  </div>
</div>

