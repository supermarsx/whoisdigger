<table id="singlewhoisTableWhoisinfo" class="table is-fullwidth is-hidden">
  <tr>
    <td colspan="4" class="is-expanded" style="border-bottom: none; text-align: right">
      <button data-notif="singlewhoisTableWhoisinfo" class="delete"></button>
    </td>
  </tr>
  <tr>
    <th>Domain:</th>
    <td class="is-expanded"><a id="singlewhoisTdDomain" href="#">n/a</a></td>
    <th>Update Date:</th>
    <td id="singlewhoisTdUpdate" class="is-expanded">n/a</td>
  </tr>
  <tr>
    <th>Registrar:</th>
    <td id="singlewhoisTdRegistrar" class="is-expanded">n/a</td>
    <th>Creation Date:</th>
    <td id="singlewhoisTdCreation" class="is-expanded">n/a</td>
