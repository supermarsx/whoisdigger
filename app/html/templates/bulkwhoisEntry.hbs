<div class='columns'>
  <div class='column'>
    <a id='bwEntryButtonFile' class='button is-large box-shadow-5'>
      <span class='icon is-large'>
        <i class='fas fa-file-upload'></i>
      </span>
      <span>Use wordlist file (.txt)</span>
    </a>
  </div>
  <div class='column'>
    <a id='bwEntryButtonWordlist' class='button is-large box-shadow-5'>
      <span>Use an input wordlist</span>
      <span class='icon is-large'>
        <i class='fas fa-edit'></i>
      </span>
    </a>
  </div>
</div>
<div id='bwExportMessageError' class='notification is-danger box-shadow-5 is-hidden'>
  <button data-notif='bwExportMessageError' class='delete'></button>
  Export failed:
  <span id='bwExportErrorText'></span>
</div>
